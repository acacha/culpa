{
    "name": "rmasters/culpa",
    "description": "Adds Blameable support to Eloquent models in Laravel",
    "keywords": ["laravel", "blameable", "created-by"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Masters",
            "email": "ross@rossmasters.com"
        }
    ],
    "homepage": "https://github.com/rmasters/culpa",
    "support": {
        "issues": "https://github.com/rmasters/culpa/issues",
        "wiki": "https://github.com/rmasters/culpa/wiki"
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "illuminate/config": "4.0.x",
        "illuminate/events": "4.0.x"
    },
    "require-dev": {
        "illuminate/database": "4.0.x",
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "mockery/mockery": "0.8.*"
    },
    "autoload": {
        "psr-0": {
            "Culpa": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
